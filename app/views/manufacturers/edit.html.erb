<div class="container">
	<h2>メーカー編集</h2>
	<hr>
	<div class="row">
		<div class="col-xs-3">
			<%= notice %>
			<%= form_for(@manufacturer) do |f| %>
			<%= f.text_field :manufacturer, autofocus: true, class:"form-control", autocomplete: "on" %>
			<%= f.submit "更新", class:"btn btn-primary btn-sm" %>
			<% end %>
			<%= link_to "戻る", new_manufacturer_path %>
		</div>
	</div>
</div>