<div class="container">
	<h2>ジャンル編集</h2>
	<hr>
	<div class="row">
		<div class="col-xs-3">
			<%# どのジャンルに属しているかの表示 %>
			<%= form_for @genre, url: rough_genre_detail_genre_path(@rough_genre.id, @genre.id), method: :patch do |f| %>
			<%= f.text_field :genre, autofocus: true, class:"form-control", autocomplete: "on" %>
			<%= f.submit "更新" %>
			<%= end %>
			<%= link_to "戻る", :back %>
		</div>
	</div>
</div>